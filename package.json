{
  "name": "sample_code_automation",
  "version": "1.0.0",
  "description": "Sample code for automation",
  "main": "index.js",
  "scripts": {
    "___________________[Build]": "____________________________________________",
    "build": "ts-node src/main.ts",
    "___________________[Docs]": "____________________________________________",
    "build-docs": "typedoc --tsconfig ./config/tsconfig.json --options ./config/typedoc.json --readme README-DOCS.md",
    "___________________[Jest]": "____________________________________________",
    "jtest": "jest --config ./config/jest.config.js",
    "jinit": "jest --init --config=./config/jest.config.js",
    "___________________[Utils]": "____________________________________________",
    "lint": "eslint -f unix \"src/**/*.{ts,tsx}\"",
    "bump": "npm version major --major"
  },
  "keywords": [
    "automation"
  ],
  "author": "tmslpm",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.14.2",
    "@types/node": "^18.11.18",
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.1",
    "typescript": "^4.9.4",
    "ts-jest": "^29.1.1",
    "jest": "^29.3.1"
  }
}
